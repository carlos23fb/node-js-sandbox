<%- include("template/header", {titulo_web: "Pets" }) %>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Desc</th>
                    <th scope="col">emoji</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>

                <% if (arrayPets.length > 0) { %>
                    
                    <% for( let index = 0; index < arrayPets.length; index++ ) { %>
                        <tr>
                            <th scope="row"><%= arrayPets[index]._id %></th>
                            <td><%= arrayPets[index].nombre %> </td>
                            <td><%= arrayPets[index].descripcion %> </td>
                            <td><%= arrayPets[index].emoji %> </td>
                            <td></td>
                        </tr>
                    <% } %>
                    

                <% } %>
            </tbody>
        </table>

    </div>
    <%- include("template/footer")%>